{
  "name": "reflect.net",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-next": "NEXT_PUBLIC_FIREBASE_STAGING=1 next dev",
    "dev-next-local": "NEXT_PUBLIC_USE_FUNCTIONS_EMULATOR=1 npm run dev-next",
    "dev-worker": "npm run dev-worker-prod",
    "dev-worker-sandbox": "npm run wrangler-sandbox -- dev",
    "dev-worker-preview": "npm run wrangler-preview -- dev",
    "dev-worker-prod": "npm run wrangler-prod -- dev",
    "reset-data-preview": "./write-secret.sh CLEAN_ROOM_UID $RANDOM preview",
    "reset-data-sandbox": "./write-secret.sh CLEAN_ROOM_UID $RANDOM sandbox",
    "reset-data-prod": "./write-secret.sh CLEAN_ROOM_UID $RANDOM",
    "nuke-it-from-orbit": "rm -rf node_modules package-lock.json .wrangler .mf .next .vercel; npm i",
    "build": "next build",
    "lint": "next lint",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check *",
    "check-types": "tsc -p tsconfig.json --noEmit && tsc -p demo/worker/tsconfig.json --noEmit",
    "publish-worker-preview": "npm run wrangler-preview -- publish",
    "publish-worker-sandbox": "npm run wrangler-sandbox -- publish",
    "publish-worker-prod": "npm run wrangler-prod -- publish",
    "worker-logs-sandbox": "npm run wrangler-sandbox -- tail",
    "worker-logs-prod": "npm run wrangler-prod -- tail",
    "wrangler-preview": "npx wrangler --config wrangler.toml --env=preview",
    "wrangler-sandbox": "npx wrangler --config wrangler.toml --env=sandbox",
    "wrangler-prod": "npx wrangler --config wrangler.toml",
    "update-reflect": "./build-latest-reflect.sh",
    "update-reflect-pull": "./build-latest-reflect.sh pull",
    "generate-puzzle-pieces": "node raw-assets/puzzle-pieces/generate-puzzle-pieces.js"
  },
  "dependencies": {
    "@rocicorp/logger": "^5.2.1",
    "@rocicorp/reflect": "0.38.0",
    "firebase": "9.23.0",
    "firebaseui": "^6.0.2",
    "nanoid": "^4.0.2",
    "nextjs-google-analytics": "^2.3.3",
    "react-confetti-explosion": "^2.1.2",
    "react-firebase-hooks": "^5.1.1",
    "react-intersection-observer": "^9.4.3",
    "react-range": "^1.8.14",
    "react-syntax-highlighter": "^15.5.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.8.0",
    "@datadog/browser-logs": "^4.11.3",
    "@next/font": "13.1.5",
    "@rocicorp/eslint-config": "^0.5.1",
    "@rocicorp/prettier-config": "^0.2.0",
    "@rocicorp/rails": "^0.5.0-beta.0",
    "@rocicorp/resolver": "^1.0.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/react-scroll": "^1.8.6",
    "@types/react-slider": "^1.3.1",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/styled-components": "^5.1.26",
    "@vercel/node": "^2.8.13",
    "buffer": "^5.7.1",
    "classnames": "^2.3.2",
    "eslint-config-next": "^13.5.4",
    "miniflare": "^3.20230717.0",
    "mirror-protocol": "0.0.0",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "next": "13.1.5",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "puppeteer": "^19.9.0",
    "react": ">=16.0 <19.0",
    "react-dom": ">=16.0 <19.0",
    "react-draggable": "^4.4.5",
    "react-hotkeys": "^2.0.0",
    "react-scroll": "^1.8.9",
    "shared": "0.0.0",
    "styled-components": "^5.3.6",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vercel": "^28.12.7",
    "zod": "^3.21.4"
  },
  "eslintConfig": {
    "extends": [
      "@rocicorp/eslint-config",
      "next/core-web-vitals"
    ]
  },
  "prettier": "@rocicorp/prettier-config"
}
