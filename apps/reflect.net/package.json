{
  "name": "reflect.net",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-next": "./keepalive.sh NODE_ENV=development next dev",
    "dev-worker": "npm run dev-worker-prod -- --experimental-local",
    "dev-worker-staging": "./keepalive.sh npm run wrangler-staging -- dev",
    "dev-worker-prod": "./keepalive.sh npm run wrangler-prod -- dev",
    "reset-data-staging": "./write-secret.sh CLEAN_ROOM_UID $RANDOM staging",
    "reset-data-prod": "./write-secret.sh CLEAN_ROOM_UID $RANDOM",
    "nuke-it-from-orbit": "rm -rf node_modules package-lock.json .wrangler .mf .next .vercel; npm i",
    "build": "next build",
    "lint": "next lint",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}'",
    "check-types": "tsc -p tsconfig.json --noEmit && tsc -p demo/worker/tsconfig.json --noEmit",
    "publish-worker-staging": "npm run wrangler-staging -- publish",
    "publish-worker-prod": "npm run wrangler-prod -- publish",
    "worker-logs-staging": "npm run wrangler-staging -- tail",
    "worker-logs-prod": "npm run wrangler-prod -- tail",
    "wrangler-staging": "npx wrangler --config wrangler-staging.toml",
    "wrangler-prod": "npx wrangler --config wrangler.toml",
    "update-reflect": "./build-latest-reflect.sh",
    "update-reflect-pull": "./build-latest-reflect.sh pull",
    "generate-puzzle-pieces": "node raw-assets/puzzle-pieces/generate-puzzle-pieces.js"
  },
  "dependencies": {
    "@rocicorp/logger": "^2.2.0",
    "@rocicorp/reflect": "*",
    "@rocicorp/reflect-server": "*",
    "nanoid": "^3.3.4",
    "nextjs-google-analytics": "^2.3.3",
    "react-confetti-explosion": "^2.1.2",
    "react-intersection-observer": "^9.4.3",
    "react-range": "^1.8.14",
    "react-syntax-highlighter": "^15.5.0",
    "replicache-react": "^2.10.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@datadog/browser-logs": "^4.11.3",
    "@next/font": "13.1.5",
    "@rocicorp/eslint-config": "^0.2.0",
    "@rocicorp/prettier-config": "^0.1.1",
    "@rocicorp/rails": "^0.4.0",
    "@rocicorp/resolver": "^1.0.0",
    "@types/mocha": "^10.0.1",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-scroll": "^1.8.6",
    "@types/react-slider": "^1.3.1",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/styled-components": "^5.1.26",
    "@vercel/node": "^2.8.13",
    "buffer": "^5.7.1",
    "classnames": "^2.3.2",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "mocha": "^10.1.0",
    "next": "13.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "puppeteer": "^19.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.5",
    "react-hotkeys": "^2.0.0",
    "react-scroll": "^1.8.9",
    "styled-components": "^5.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vercel": "^28.12.7",
    "zod": "^3.21.4"
  },
  "eslintConfig": {
    "extends": [
      "@rocicorp/eslint-config",
      "next/core-web-vitals"
    ]
  },
  "prettier": "@rocicorp/prettier-config"
}
