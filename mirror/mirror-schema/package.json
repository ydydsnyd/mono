{
  "name": "mirror-schema",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "module": "./src/mod.ts",
  "types": "./src/mod.ts",
  "scripts": {
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "check-types": "tsc --noEmit",
    "format": "prettier --write *",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "deploy-rules-staging": "npx firebase-tools --project staging deploy --only firestore:rules",
    "deploy-rules-prod": "npx firebase-tools --project prod deploy --only firestore:rules",
    "build": "unbuild"
  },
  "dependencies": {
    "@badrap/valita": "^0.2.0"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.4.2",
    "@rocicorp/prettier-config": "^0.1.1",
    "@types/jest": "^29.4.2",
    "shared": "0.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1"
  },
  "peerDependencies": {
    "firebase-tools": "^12.3.0"
  },
  "exports": {
    ".": "./dist/mod.mjs",
    "./agent.js": "./dist/agent.mjs",
    "./app-key.js": "./dist/app-key.mjs",
    "./app.js": "./dist/app.mjs",
    "./cloudflare.js": "./dist/cloudflare.mjs",
    "./deployment.js": "./dist/deployment.mjs",
    "./membership.js": "./dist/membership.mjs",
    "./server.js": "./dist/server.mjs",
    "./team.js": "./dist/team.mjs",
    "./timestamp.js": "./dist/timestamp.mjs",
    "./user.js": {
      "import": "./dist/user.mjs",
      "types": "./dist/user.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
