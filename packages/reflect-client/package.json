{
  "name": "reflect-client",
  "description": "",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write *",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "check-types": "tsc --noEmit",
    "build-dts": "rm -rf out/.dts/ && tsc --emitDeclarationOnly --outDir out/.dts/ && rollup --config rollup.config.js && rm -rf out/.dts",
    "build": "rm -rf out && npm run build-dts"
  },
  "dependencies": {
    "@badrap/valita": "^0.3.0",
    "@rocicorp/lock": "^1.0.4",
    "@rocicorp/logger": "^5.2.2",
    "@rocicorp/resolver": "^1.0.2"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.5.1",
    "@rocicorp/prettier-config": "^0.2.0",
    "@types/sinon": "^10.0.11",
    "datadog": "0.0.0",
    "esbuild": "^0.20.2",
    "playwright": "^1.43.1",
    "reflect-protocol": "0.0.0",
    "reflect-shared": "0.0.0",
    "replicache": "15.0.1",
    "rollup": "^4.19.0",
    "shared": "0.0.0",
    "sinon": "^13.0.1",
    "typescript": "^5.5.3"
  },
  "exports": {
    ".": "./src/mod.ts"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
