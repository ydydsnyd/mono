{
  "name": "shared",
  "private": true,
  "type": "module",
  "module": "./src/mod.ts",
  "types": "./src/mod.ts",
  "scripts": {
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "check-types": "tsc --noEmit",
    "format": "prettier --write *",
    "check-format": "prettier --check *",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/"
  },
  "devDependencies": {
    "@badrap/valita": "^0.2.0",
    "@rocicorp/eslint-config": "^0.3.0",
    "@rocicorp/prettier-config": "^0.1.1",
    "@rocicorp/logger": "^4.1.0",
    "@types/jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "exports": {
    ".": {
      "import": "./src/mod.ts",
      "types": "./src/mod.ts"
    },
    "./valita.js": {
      "import": "./src/valita.ts",
      "types": "./src/valita.ts"
    },
    "./must.js": {
      "import": "./src/must.ts",
      "types": "./src/must.ts"
    },
    "./has-own.js": {
      "import": "./src/has-own.ts",
      "types": "./src/has-own.ts"
    },
    "./asserts.js": {
      "import": "./src/asserts.ts",
      "types": "./src/asserts.ts"
    },
    "./json.js": {
      "import": "./src/json.ts",
      "types": "./src/json.ts"
    },
    "./deep-clone.js": {
      "import": "./src/deep-clone.ts",
      "types": "./src/deep-clone.ts"
    },
    "./config.js": {
      "import": "./src/config.ts",
      "types": "./src/config.ts"
    },
    "./buffer-sizer.js": {
      "import": "./src/buffer-sizer.ts",
      "types": "./src/buffer-sizer.ts"
    }
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config"
}
