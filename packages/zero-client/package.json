{
  "name": "zero-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "test": "npm run test-chromium && npm run test-firefox && npm run test-webkit",
    "test-chromium": "vitest run --browser.name=chromium",
    "test-firefox": "vitest run --browser.name=firefox",
    "test-webkit": "vitest run --browser.name=webkit",
    "test:watch": "vitest",
    "bench": "vitest bench",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch"
  },
  "dependencies": {
    "@rocicorp/lock": "^1.0.4",
    "@rocicorp/logger": "^5.3.0",
    "@rocicorp/resolver": "^1.0.2"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.5.1",
    "@rocicorp/prettier-config": "^0.2.0",
    "@types/sinon": "^10.0.11",
    "datadog": "0.0.0",
    "esbuild": "^0.20.2",
    "replicache": "15.2.1",
    "shared": "0.0.0",
    "sinon": "^13.0.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "zero-protocol": "0.0.0"
  },
  "eslintConfig": {
    "extends": "../../eslint-config.json"
  },
  "prettier": "@rocicorp/prettier-config"
}
