{
  "name": "@rocicorp/zero",
  "version": "0.5.2024110200+4ba53b",
  "scripts": {
    "build": "rm -rf out && npm run build-server && npm run build-client",
    "build-client": "tsc -p tsconfig.client.json && tsc-alias -p tsconfig.client.json && node tool/build.js",
    "build-server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json && chmod +x out/zero/src/cli.js",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/"
  },
  "dependencies": {
    "@badrap/valita": "^0.3.11",
    "@databases/escape-identifier": "^1.0.3",
    "@databases/sql": "^3.3.0",
    "@drdgvhbh/postgres-error-codes": "^0.0.6",
    "@fastify/cors": "^10.0.0",
    "@fastify/websocket": "^11.0.0",
    "@google-cloud/precise-date": "^4.0.0",
    "@postgresql-typed/oids": "^0.2.0",
    "@rocicorp/lock": "^1.0.4",
    "@rocicorp/logger": "^5.3.0",
    "@rocicorp/resolver": "^1.0.2",
    "@rocicorp/zero-sqlite3": "^1.0.3",
    "chalk-template": "^1.1.0",
    "command-line-args": "^6.0.1",
    "command-line-usage": "^7.0.3",
    "compare-utf8": "^0.1.1",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "fastify": "^5.0.0",
    "jose": "^5.9.3",
    "json-custom-numbers": "^3.1.1",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.2",
    "lodash.snakecase": "^4.1.1",
    "pg": "^8.11.3",
    "pg-format": "npm:pg-format-fix@^1.0.5",
    "pg-logical-replication": "^2.0.7",
    "postgres": "^3.4.4",
    "postgres-array": "^3.0.2",
    "semver": "^7.5.4",
    "strip-ansi": "^7.1.0",
    "url-pattern": "^1.0.3",
    "tsx": "^4.19.1",
    "ws": "^8.18.0",
    "xxhash-wasm": "^1.0.2"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.5.1",
    "@rocicorp/prettier-config": "^0.2.0",
    "esbuild": "^0.20.2",
    "replicache": "15.2.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  },
  "type": "module",
  "main": "out/zero.js",
  "module": "out/zero.js",
  "types": "out/zero-client/src/mod.d.ts",
  "exports": {
    ".": {
      "types": "./out/zero-client/src/mod.d.ts",
      "default": "./out/zero.js"
    },
    "./react": {
      "types": "./out/zero-react/src/mod.d.ts",
      "default": "./out/react.js"
    },
    "./solid": {
      "types": "./out/zero-solid/src/mod.d.ts",
      "default": "./out/solid.js"
    },
    "./server": {
      "types": "./out/zero-cache/src/server/main.d.ts",
      "default": "./out/zero-cache/src/server/main.js"
    },
    "./config": {
      "types": "./out/zero/src/config.d.ts",
      "default": "./out/zero/src/config.js"
    },
    "./advanced": {
      "types": "./out/zero/src/advanced.d.ts",
      "default": "./out/zero/src/advanced.js"
    },
    "./schema": {
      "types": "./out/zero-schema/src/mod.d.ts",
      "default": "./out/zero-schema/src/mod.js"
    }
  },
  "bin": {
    "zero-cache": "./out/zero/src/cli.js"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "out",
    "!*.tsbuildinfo"
  ],
  "eslintConfig": {
    "extends": "../../eslint-config.json"
  },
  "prettier": "@rocicorp/prettier-config"
}
