{
  "name": "zql",
  "version": "0.0.0",
  "description": "ZQL IVM Query library",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write 'src/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check 'src/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-types": "tsc --noEmit",
    "build-dts": "rm -rf out/.dts/ && tsc --emitDeclarationOnly --outDir out/.dts/ && rollup --config rollup.config.js && rm -rf out/.dts",
    "build": "rm -rf out && node tool/build.js && npm run build-dts ",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "bench": "vitest bench",
    "test-types": "vitest run --typecheck.only --no-browser.enabled",
    "test-types:watch": "vitest watch --typecheck.only --no-browser.enabled"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.5.1",
    "@rocicorp/logger": "^5.2.2",
    "@rocicorp/prettier-config": "^0.2.0",
    "@rocicorp/rails": "^0.10.0",
    "btree": "0.0.0",
    "fast-check": "^3.18.0",
    "nanoid": "^4.0.2",
    "playwright": "^1.43.1",
    "replicache": "15.2.1",
    "shared": "0.0.0",
    "typescript": "^5.5.3",
    "wa-sqlite": "github:rhashimoto/wa-sqlite#ca2084cdd188c56532ba3e272696d0b9e21a23bf",
    "zod": "^3.21.4"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": "@rocicorp/prettier-config",
  "dependencies": {
    "compare-utf8": "^0.1.1"
  }
}
